{
    "name": "mrshanebarron/select",
    "description": "Enhanced select component using Choices.js for Laravel with Livewire, Blade, and Vue support",
    "keywords": ["laravel", "select", "choices", "dropdown", "multiselect", "autocomplete", "livewire", "blade", "vue"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shane Barron",
            "email": "mrshanebarron@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDesign\\Select\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDesign\\Select\\SelectServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
